<table class="table dashboard-table-margin">
        <thead>
          <tr>
            <th>Name</th>
            <th>Date</th>
          </tr>
        </thead>
        <tbody>
        <% @purchased.each do |sale| %> 
          <tr>
            <td><%= link_to sale.book.name, pickup_path(sale.guid) %></td>
            <td><%= time_ago_in_words(sale.created_at) %>
          </tr>
        <% end %>
        </tbody>
      </table>